
org 100h
    
    
    lea dx,msg1
    mov ah,09h 
    int 21h
    
    mov dl,20h
    mov ah,02h
    int 21h
    
    mov bh,1
    mov dh,1
    mov cx,7
    
    fibloop:
       or dl,30h
       mov ah,02h
       int 21h
       
       mov dl,dh
       mov dh,bh
       
       push dx
         mov al,dl 
         mov ah,dh
         add ah,al
         mov bh,ah
         
       pop dx
     loop fibloop
     
    msg1 db "FIBONACCI FIRST TIMES ", 0dh,0ah
         db "I START FROM 0 WHILE OTHERS LIKES",0dh,0ah
         db "TO INCLUDE THE 1 IN FIRST TEAM ." ,0dh,0ah,"$"   
    FIBTERM db 12 dup<0>
       
    
